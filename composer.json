{
  "name": "survos/wikidata",
  "description": "A PHP client for working with Wikidata API.",
  "keywords": [
    "wikidata",
    "client",
    "php"
  ],
  "homepage": "https://github.com/survos/wikidata",
  "license": "MIT",
  "authors": [
    {
      "name": "Arhey",
      "role": "developer"
    },
    {
      "name": "tacman",
      "role": "developer"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require": {
    "php": "^8.4",
    "illuminate/collections": "^11.0||^12.0",
    "symfony/http-client": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "Wikidata\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Wikidata\\Tests\\": "tests"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^2.0",
    "rector/rector": "^2.0"
  }
}
